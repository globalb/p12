<IfModule mod_rewrite.c>
	RewriteEngine on
	RewriteBase /multiscraper/
	RewriteCond $1 !^(index\.php|robots\.txt|createMSPROpackage\.php|phpinfo\.php|test\.php|proxyMyOwn\.txt|public|pma|addons|favicon\.ico|media|uploads)
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteRule ^(.*)$ index.php?/$1 [L]
</IfModule>